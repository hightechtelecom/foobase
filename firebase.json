{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "",
    "cleanUrls": true,
    "trailingSlash": false,
    "ignore": [
      "firebase.json",
      "database.rules.json",
      "firebase.indexes.json",
      "firestore.rules",
      "package.json",
      "storage.rules",
      "README.md",
      "**/.*",
      "**/node_modules/**",
      "**/archive/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  }
}
